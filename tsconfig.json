{
  "compilerOptions": {
    "target": "es2016",                                                        
    "module": "ES2020",                    
    "lib": ["es6"],                     
    "allowJs": true,
    "outDir": "build",                          
    "rootDir": ".",
    "strict": true,         
    "noImplicitAny": true,
    "esModuleInterop": true,
    "moduleResolution":"node",
  },
  "exclude": [
    "node_modules",
    "dist",
    "build",
    "output",
    "src"
  ],
}
